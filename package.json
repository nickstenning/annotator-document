{
  "name": "annotator-document",
  "version": "1.0.0",
  "description": "An extension module for Annotator that adds document metadata to annotations.",
  "main": "document.js",
  "dependencies": {
    "browserify-shim": "^3.8.5"
  },
  "devDependencies": {
    "browserify": "^9.0.8",
    "chai": "^2.2.0",
    "karma": "^0.12.31",
    "karma-browserify": "^4.1.2",
    "karma-mocha": "^0.1.10",
    "karma-phantomjs-launcher": "^0.1.4",
    "mocha": "^2.2.4"
  },
  "browser": "browser.js",
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "annotator": "global:annotator"
  },
  "scripts": {
    "test": "./node_modules/karma/bin/karma start --single-run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/openannotation/annotator-document"
  },
  "keywords": [
    "annotator",
    "annotation",
    "metadata"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/openannotation/annotator-document/issues"
  },
  "homepage": "https://github.com/openannotation/annotator-document"
}
